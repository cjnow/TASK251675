<?xml version="1.0" encoding="UTF-8"?>
<database>
    <element label="Troubleshooting" max_length="40" name="x_qune_da_troubleshooting" type="collection">
        <element hint="Select one of more Discovery Status records for Troubleshooting." label="Discovery Status" max_length="4000" name="discovery_status" reference="discovery_status" type="glide_list"/>
        <element default="javascript:global.getNextObjNumberPadded();" display="true" hint="Autogenerated." label="Number" max_length="40" name="number" read_only="true" type="string"/>
        <element choice="1" default="awaiting_input" hint="Status of Troubleshooting." label="Status" max_length="40" name="status" read_only="true" type="choice">
            <choice>
                <element label="Error: Contact QuickNexus" sequence="-10" value="error"/>
                <element label="Awaiting Input" sequence="10" value="awaiting_input"/>
                <element label="Ready for Analysis" sequence="20" value="ready_for_analysis"/>
                <element label="Analyzing Discovery Logs" sequence="30" value="analyzing_discovery_logs"/>
                <element label="Analysis Executed" sequence="40" value="analysis_executed"/>
                <element label="Incidents Generated" sequence="50" value="incidents_generated"/>
            </choice>
        </element>
    </element>
</database>
